{
  "name": "my-course-extension",
  "displayName": "my-course-extension",
  "description": "ddd",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:courseIDE.showMainInterface",
    "onCommand:courseIDE.startCollaboration"
  ],
  "contributes": {
    "commands": [
      {
        "command": "courseIDE.showMainInterface",
        "title": "显示插件主界面"
      },
      {
        "command": "courseIDE.openExistingCourse",
        "title": "打开已有课程"
      },
      {
        "command": "courseIDE.previewFile",
        "title": "预览文件"
      },
      {
        "command": "courseIDE.startCollaboration",
        "title": "开启共享编辑"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "courseDashboard",
          "title": "Course IDE",
          "icon": "images/course.png"
        }
      ]
    },
    "views": {
      "courseDashboard": [
        {
          "id": "courseView",
          "name": "课程列表"
        }
      ]
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p .",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
